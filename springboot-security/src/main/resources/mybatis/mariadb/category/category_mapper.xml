<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shm.category">	
 
 	<insert id="insert" parameterType="com.shm.category.CategoryEntity" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO CATEGORY (
			NAME
		) VALUES (
			#{name}
		)
	</insert>

	<select id="select" resultType="com.shm.category.CategoryEntity">
		SELECT
			ID,
			NAME,
			FRST_REG_DATE,
			FRST_REG_USER_ID,
			LAST_CHG_DATE,
			LAST_CHG_USER_ID
		FROM CATEGORY
	</select>
	
</mapper>
